    private void jCheckBox6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jCheckBox6ActionPerformed
        if(jCheckBox6.isSelected()){
            Double tax=0.05*(Double.parseDouble(jTextField7.getText())+Double.parseDouble(jTextField8.getText())+Double.parseDouble(jTextField9.getText()));
            jTextField10.setText(String.format("%.2f", tax));
        }
        // TODO add your handling code here:
    }//GEN-LAST:event_jCheckBox6ActionPerformed

    private void jCheckBox7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jCheckBox7ActionPerformed
        if(jCheckBox6.isSelected()){
            Double vat=0.07*(Double.parseDouble(jTextField7.getText())+Double.parseDouble(jTextField8.getText())+Double.parseDouble(jTextField9.getText()));
            jTextField11.setText(String.format("%.2f", vat));
        }
        // TODO add your handling code here:
    }//GEN-LAST:event_jCheckBox7ActionPerformed

