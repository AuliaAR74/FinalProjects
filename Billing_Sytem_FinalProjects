    private void totalcost(){
        Double total=0.0;
        int cnt=0;
        if(!jTextField2.getText().isEmpty()){
            total=total+Double.parseDouble(jTextField2.getText());
            cnt++;
        }
        if(!jTextField3.getText().isEmpty()){
            total=total+Double.parseDouble(jTextField3.getText());
            cnt++;
        }
        if(!jTextField4.getText().isEmpty()){
            total=total+Double.parseDouble(jTextField4.getText());
            cnt++;
        }
        if(!jTextField5.getText().isEmpty()){
            total=total+Double.parseDouble(jTextField5.getText());
            cnt++;
        }
        if(!jTextField6.getText().isEmpty()){
            total=total+Double.parseDouble(jTextField6.getText());
            cnt++;
        }
        jTextField7.setText(Double.toString(total)); 
         jTextField8.setText(Double.toString(Travel*cnt));
         jTextField9.setText(String.valueOf(cnt));
       
    }
    private Double operasi(Double f,Double s,String o){
        Double hasil = null;
        switch(o){
            case "*":hasil=f*s;break;
            case "/":hasil=f/s;break;
            case "+":hasil=f+s;break;
            case "-":hasil=f-s;break;
        }
        return hasil;
    }

