    private void jButton20ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton20ActionPerformed

        Double subtotal=Double.parseDouble(jTextField7.getText())+Double.parseDouble(jTextField8.getText())+Double.parseDouble(jTextField9.getText());
        jTextField12.setText(Double.toString(subtotal));
        if(!jTextField10.getText().isEmpty()){
            jTextField13.setText(jTextField10.getText());
        }
        else{
            jTextField13.setText("0");
        }
        Double total_amount=Double.parseDouble(jTextField12.getText())+Double.parseDouble(jTextField13.getText());
        jTextField14.setText(Double.toString(total_amount));
        // TODO add your handling code here:
    }//GEN-LAST:event_jButton20ActionPerformed

